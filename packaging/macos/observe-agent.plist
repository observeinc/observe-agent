<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.observeinc.agent</string>
    <key>ProgramArguments</key>
    <array>
        <string>/usr/local/libexec/observe-agent</string>
        <string>start</string>
    </array>
    <key>KeepAlive</key>
    <true/>
    <key>StandardOutPath</key>
    <string>/var/log/observe-agent.log</string>
    <key>StandardErrorPath</key>
    <string>/var/log/observe-agent.log</string>
</dict>
</plist>
